{
    "name": "sergiohermes/laminas-oauth2-doctrine-mutatetablenames",
    "description": "Module for Laminas API Tools Doctrine OAuth2 Server Adapter so table names can be configured custom",
    "license": "MIT",
    "keywords": [
        "oauth2",
        "doctrine",
        "laminas-api-tools"
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "laminas/laminas-modulemanager": "^2.7",
        "laminas/laminas-stdlib": "^2.7 || ^3.0",
        "api-skeletons/oauth2-doctrine": "^4.0 || ^5.0",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "LaminasApi\\OAuth2\\Doctrine\\MutateTableNames\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.8 || ^5.2",
        "squizlabs/php_codesniffer": "^2.7",
        "satooshi/php-coveralls": "~1.0"
    }
}
